{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "APIs": {
    "Weather": {
      "Url": "https://example.com/weather",
      "ArrayPath": "items"
    },
    "News": {
      "Url": "https://example.com/news",
      "ArrayPath": "news"
    },
    "Github": {
      "Url": "https://example.com/github",
      "ArrayPath": "repos"
    },
    "Netflix": {
      "Url": "https://example.com/netfix",
      "ArrayPath": "movies"
    }
  },
  "JwtSettings": {
    "Issuer": "issuer",
    "Audience": "audience",
    "Key": "AveryLongSecretKeyThatIsAtLeast128BitsLong"
  }
}
